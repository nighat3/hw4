<div class="row">
  <% for post in @entries %>
    <div class="col-sm-6 col-md-3">
      <h4>
        <% if post.user and post.user_id == @current_user["id"]  %>
          <%= post.user["username"]%>
          <%= post.user["body"]%>
          <% if post.uploaded_image.attached?%>
          <img src="<%= url_for(post.uploaded_image) %>" class="img-fluid">
          <div class="col text-end">
          <div class="small fst-italic"><%= post["created_at"].strftime("%-m/%d/%y at %-I:%M %p") %></div>
        </div>
        <% else %>
          <h4> No image </h4>
        <% end %>

        <% end %>
      </h4>

     

      
      
    </div>
  <% end %>
</div>


<p>
  <a href="/entries/new?place_id=<%= @place["id"] %>" class="btn btn-success">New Entry</a>
</p>

<p>
  <a href="/places" class="btn btn-success">Back to All Places</a>
</p>


